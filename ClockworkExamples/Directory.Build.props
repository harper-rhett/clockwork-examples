<Project>
	<PropertyGroup>
		<UseLocalEngine>false</UseLocalEngine>
		<LocalEnginePath>..\..\..\clockwork-engine\ClockworkEngine\ClockworkEngine.csproj</LocalEnginePath>
	</PropertyGroup>

	<ItemGroup Condition="'$(UseLocalEngine)' == 'false'">
		<PackageReference Include="ClockworkEngine" Version="0.2.5" />
